{
  "expo": {
    "name": "quotely-codebase",
    "slug": "quotely-codebase",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Quotely-favicon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/Quotely-favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#fffff"
    },
    "ios": {
      "supportsTablet": false,
      "usesAppleSignIn": true,
      "infoPlist": {
        "NSUserTrackingUsageDescription": "Deze app gebruikt advertentie-ID's om gepersonaliseerde advertenties te tonen.",
        "LSApplicationQueriesSchemes": [
          "fb",
          "instagram",
          "twitter",
          "tiktoksharesdk"
        ]
      },
      "bundleIdentifier": "com.anonymous.quotelycodebase"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.anonymous.quotelycodebase"
    },
    "plugins": [

    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0c0c7f5f-1396-4ed9-9365-d26bbff502a2"
      }
    }
  }
}
